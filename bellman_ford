def bellman_ford(graph, start):
    dist = {v: float('inf') for v in graph}
    dist[start] = 0
    for _ in range(len(graph)-1):
        for u in graph:
            for v, w in graph[u]:
                if dist[u] + w < dist[v]:
                    dist[v] = dist[u] + w
    for u in graph:
        for v, w in graph[u]:
            if dist[u] + w < dist[v]:
                return "Graph contains a negative weight cycle"
    return dist

graph = {
    "A": [("B", 4), ("C", 2)],
    "B": [("C", 3), ("D", 2), ("E", 3)],
    "C": [("B", 1), ("D", 4), ("E", 5)],
    "D": [("E", -5)],
    "E": []
}

print(bellman_ford(graph, "A"))
