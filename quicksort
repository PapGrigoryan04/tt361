arr = []
n = int(input("array length: "))
for i in range(n):
    x = int(input("enter array element"))
    arr.append(x)

def qsort(a):
    if len(a) <= 1:
        return a
    pivot = a[len(a) // 2]
    left = [x for x in a if x < pivot]
    mid = [x for x in a if x == pivot]
    right = [x for x in a if x > pivot]
    return qsort(left) + mid + qsort(right)

arr = qsort(arr)
print(arr)
